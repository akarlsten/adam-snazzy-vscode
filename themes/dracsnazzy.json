      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "HTML/CSS attribute names",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#5af78e",
        "fontStyle": "italic"
      }
